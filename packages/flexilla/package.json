{
  "name": "@flexilla/flexilla",
  "private": false,
  "version": "2.2.8",
  "type": "module",
  "description": "A set of unstyled interactive components for building interactive, and customizable user interfaces. Open-Source",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/flexilla.js",
  "module": "./dist/flexilla.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/flexilla.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/flexilla.umd.cjs"
      }
    },
    "./flexilla.css": "./dist/flexilla.css",
    "./dropdown.css": "./dist/dropdown.css",
    "./modal.css": "./dist/modal.css",
    "./offcanvas.css": "./dist/offcanvas.css",
    "./tabs.css": "./dist/tabs.css",
    "./tooltip.css": "./dist/tooltip.css"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "!vite.config.ts",
    "!.gitignore",
    "!tsconfig.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@flexilla/accordion": "^2.2.5",
    "@flexilla/auto-resize-area": "^2.0.14",
    "@flexilla/collapse": "^2.1.5",
    "@flexilla/custom-range": "^2.0.13",
    "@flexilla/dismissible": "^2.0.14",
    "@flexilla/dropdown": "^2.2.6",
    "@flexilla/modal": "^2.3.4",
    "@flexilla/offcanvas": "^2.1.4",
    "@flexilla/pin-input": "^0.0.1",
    "@flexilla/popover": "^1.0.3",
    "@flexilla/tabs": "^2.2.3",
    "@flexilla/tooltip": "^2.4.3"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "Flexilla",
    "interactive component",
    "components library",
    "ts",
    "js",
    "ui component",
    "interactive",
    "open-source"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unoforge/flexilla.git"
  },
  "bugs": {
    "url": "https://github.com/unoforge/flexilla/issues"
  },
  "license": "MIT",
  "author": "johnkat-mj"
}
