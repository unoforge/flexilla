{
  "name": "@flexilla/accordion",
  "private": false,
  "version": "2.1.5",
  "type": "module",
  "description": "A versatile and interactive accordion component for creating collapsible sections in web applications, conserving space and improving user experience",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/accordion.umd.cjs",
  "module": "./dist/accordion.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/accordion.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/accordion.umd.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "!vite.config.ts",
    "!.gitignore",
    "!tsconfig.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@flexilla/manager": "latest",
    "@flexilla/utilities": "^2.1.2"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3"
  },
  "keywords": [
    "accordion",
    "js",
    "ts",
    "collapsible",
    "component",
    "interactive",
    "intercative component",
    "Flexilla"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/unoforge/flexilla"
  },
  "bugs": {
    "url": "https://github.com/unoforge/flexilla/issues"
  },
  "license": "MIT",
  "author": "johnkat-mj"
}
