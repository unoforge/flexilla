{
  "name": "@flexilla/utilities",
  "private": false,
  "version": "2.2.0",
  "type": "module",
  "description": "Utilities package for flexilla library",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/utilities.js",
  "module": "./dist/utilities.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/utilities.js"
      },
      "require": {
        "types": "./dist/utilities.d.ts",
        "default": "./dist/utilities.cjs"
      }
    },
    "./accessibility": {
      "import": {
        "types": "./dist/accessibility.js",
        "default": "./dist/accessibility.d.ts"
      },
      "require": {
        "types": "./dist/accessibility.d.ts",
        "default": "./dist/accessibility.cjs"
      }
    },
    "./toggler": {
      "import": {
        "types": "./dist/toggler.d.ts",
        "default": "./dist/toggler.js"
      },
      "require": {
        "types": "./dist/toggler.d.ts",
        "default": "./dist/toggler.cjs"
      }
    },
    "./selector": {
      "import": {
        "types": "./dist/selector.d.ts",
        "default": "./dist/selector.js"
      },
      "require": {
        "types": "./dist/selector.d.ts",
        "default": "./dist/selector.cjs"
      }
    },
    "./dom-utilities": {
      "import": {
        "types": "./dist/dom-utilities.d.ts",
        "default": "./dist/dom-utilities.js"
      },
      "require": {
        "types": "./dist/dom-utilities.d.ts",
        "default": "./dist/dom-utilities.cjs"
      }
    },
    "./scroll-to-top": {
      "import": {
        "types": "./dist/scroll-to-top.d.ts",
        "default": "./dist/scroll-to-top.js"
      },
      "require": {
        "types": "./dist/scroll-to-top.d.ts",
        "default": "./dist/scroll-to-top.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json",
    "!vite.config.ts",
    "!.gitignore",
    "!tsconfig.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3"
  },
  "keywords": [
    "flexilla",
    "flexilla utilities"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/unoforge/flexilla"
  },
  "bugs": {
    "url": "https://github.com/unoforge/flexilla/issues"
  },
  "license": "MIT",
  "author": "johnkat-mj"
}
